{
    "manifest_version": 3,
    "name": "VTuber: Pump.Fun Live Streaming",
    "version": "0.0.0.3",
    "description": "Enables live streaming of Pump.Fun coins",
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon-16.png",
            "48": "icons/icon-48.png",
            "128": "icons/icon-128.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["*://pump.fun/*"],
            "js": ["content.js"]
        }
    ]
}